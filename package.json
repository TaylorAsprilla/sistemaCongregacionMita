    "ng": "ng",
    "start:dev": "ng serve",
    "start:production": "ng s --configuration production",
    "start:qa": "ng s --configuration qa",
    "build": "ng build",
    "clean": "rm -rf dist",
    "clean-modules": "rm -rf node_modules package-lock.json",
    "build:prod": "ng build --configuration production --base-href",
    "build:qa": "ng build --configuration qa --base-href",
    "deploy-front:prod": "rm -rf dist && ng build --configuration production --base-href && aws s3 sync dist s3://sistemacmi --acl public-read",
